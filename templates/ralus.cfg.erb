# ralus.cfg
# managed by Puppet
#
Software\Symantec\Backup Exec For Windows\Agent Browser\TcpIp\AdvertisementPort=<%= @advertisement_port %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Debug\AgentConfig=<%= @agent_config %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Debug\VXBSAlevel=<%= @vxbsa_level %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Advertise All=<%= @advertise_all %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Advertise Now=<%= @advertise_now %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Advertisement Purge=<%= @advertisement_purge %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Advertising Disabled=<%= @advertising_disabled %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Advertising Interval Minutes=<%= @advertising_interval_minutes %>
<% @agent_directory_list.to_enum.with_index(1).each do |agent, idx| -%>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Agent Directory List_<%= idx %>= <%= agent %>
<% end -%>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Agents\Auto Discovery Enabled=<%= @auto_discovery_enabled %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\Logging\RANT NDMP Debug Level=<%= @rant_ndmp_debug_level %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RALUS\DisableOFO=<%= @disable_ofo %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RALUS\Encoder=<%= @encoder %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RALUS\ShowTSAFS=<%= @show_tsafs %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RALUS\VfmPath=<%= @vfm_path %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RMAL\DisableRMAL=<%= @disable_rmal %>
Software\Symantec\Backup Exec For Windows\Backup Exec\Network\TcpIp\DnsQueryTimeOut=<%= @dnsquery_timeout %>
# Excludes
<% @systemfs_type_exclude.to_enum.with_index(1).each do |fstype, idx| -%>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RALUS\SystemFSTypeExclude<%= idx %>=<%= fstype %>
<% end -%>
<% @system_exclude.to_enum.with_index(1).each do |exclude, idx| -%>
Software\Symantec\Backup Exec For Windows\Backup Exec\Engine\RALUS\SystemExclude<%= idx %>=<%= exclude %>
<% end -%>
